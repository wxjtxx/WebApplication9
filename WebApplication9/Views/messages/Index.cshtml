

@model IEnumerable<Blog.Models.message>

@{

    Layout = "~/Views/Shared/_Layout1.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
       
    </p>
    <table class="table">
        <h4>留言</h4>
        <tr>
            <th>
  内容
</th>
            <th>
                
                发表时间
            </th>
            
          
            <th>
               接收人
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
    @Html.DisplayFor(modelItem => item.content)
</td>
                <td>
                    @Html.DisplayFor(modelItem => item.message_time)
                </td>
                
               
                <td>
                    @Html.DisplayFor(modelItem => item.UserInfo1.User_name)
                </td>
                <td>
                    @Html.ActionLink("新建", "Create")
                    @Html.ActionLink("返回", "Edit", new { id = item.message_id }) |
                    @Html.ActionLink("详细", "Details", new { id = item.message_id }) |
                    @Html.ActionLink("删除", "Delete", new { id = item.message_id })
                </td>
            </tr>
        }

    </table>
</body>
</html>
