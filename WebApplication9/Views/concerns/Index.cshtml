


@model IEnumerable<Blog.Models.concern>

@{

    Layout = "~/Views/Shared/_Layout1.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
   
    <table class="table">
        <tr>
          
            <th>
              
                关注者
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
               <td>
                    @Html.DisplayFor(modelItem => item.UserInfo1.User_name)
                </td>
                <td>
                    @Html.ActionLink("编辑", "Edit", new { id = item.concern_id }) |
                    @Html.ActionLink("详细", "Details", new { id = item.concern_id }) |
                    @Html.ActionLink("删除", "Delete", new { id = item.concern_id })
                </td>
            </tr>
        }

    </table>
</body>
</html>
